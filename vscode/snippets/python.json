{
  /**
   ** Mypy.
   */
  "Pytest Mark Skip": {
    "prefix": ["sk"],
    "body": ["@pytest.mark.skip"]
  },

  "Pytest Mark Db": {
    "prefix": ["db"],
    "body": ["@pytest.mark.django_db"]
  },

  /**
   ** Mypy.
   */

  "Type Ignore": {
    "prefix": ["ti"],
    "body": ["# type: ignore"]
  },

  /**
   ** Yapf.
   */

  "Yapf Disable": {
    "prefix": "yd",
    "body": ["# yapf: disable"]
  },

  /**
   ** Debugger.
   */

  "Breakpoint": {
    "prefix": ["br"],
    "body": ["breakpoint()"]
  },

  "ipython debugger": {
    "prefix": ["pdb"],
    "body": ["__import__('ipdb').set_trace()"]
  },

  /**
   ** Print.
   */

  "Print": {
    "prefix": ["p"],
    "body": ["print($1)"]
  },

  "Print Equals": {
    "prefix": ["p="],
    // 91m - red
    // 92m - green
    // 93m - yellow
    // 94m - blue
    // 95m - purple
    // 96m - cyan
    // 97m - white
    // "body": ["print(f\"{$1=}\")"]
    //"body": ["print(f\"\\033[97m{$1=}\\033[00m\")"]
    "body": ["print(f\"{$1=}\")"]
  },

  "Print Object": {
    "prefix": ["po"],
    "body": ["print(\"${1:object} :>> \", ${1:object})"]
  },

  "Pretty Print": {
    "prefix": ["pp"],
    "body": ["__import__('pprint').pprint($1)"]
  },

  "Print SQL": {
    "prefix": ["psql"],
    "body": [
      "print(__import__('sqlparse').format(str($1), reindent=True, keyword_case='upper'))"
    ]
  }
}
