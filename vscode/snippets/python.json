{
  "type ignore": {
    "prefix": ["tig"],
    "body": ["# type: ignore"],
  },

  "yapf disable": {
    "prefix": "yd",
    "body": ["# yapf: disable"]
  },

  "ipython debugger": {
    "prefix": ["ipdb"],
    "body": ["__import__('ipdb').set_trace()"]
  },

  "print": {
    "prefix": ["p"],
    "body": ["print(f'{$1=}')"]
  },

  "pretty print": {
    "prefix": ["pp"],
    "body": ["__import__('pprint').pprint($1)"]
  }
}
