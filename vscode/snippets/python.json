{
  //* mypy.
  "type ignore": {
    "prefix": ["tig"],
    "body": ["# type: ignore"]
  },

  //* yapf.
  "yapf disable": {
    "prefix": "yd",
    "body": ["# yapf: disable"]
  },

  //* debugger.
  "breakpoint": {
    "prefix": ["br"],
    "body": ["breakpoint()"]
  },

  "ipython debugger": {
    "prefix": ["pdb"],
    "body": ["__import__('ipdb').set_trace()"]
  },

  //* print.
  "print": {
    "prefix": ["p"],
    "body": ["print($1)"]
  },

  "print equals": {
    "prefix": ["p="],
    "body": ["print(f\"{$1=}\")"]
  },

  "print object": {
    "prefix": ["po"],
    "body": ["print(\"${1:object} :>> \", ${1:object})"]
  },

  "pretty print": {
    "prefix": ["pp"],
    "body": ["__import__('pprint').pprint($1)"]
  },

  "print sql": {
    "prefix": ["psql"],
    "body": [
      "print(__import__('sqlparse').format(str($1), reindent=True, keyword_case='upper'))"
    ]
  },

}
