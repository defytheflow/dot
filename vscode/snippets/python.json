{
  "type ignore": {
    "prefix": ["tig"],
    "body": ["# type: ignore"]
  },

  "yapf disable": {
    "prefix": "yd",
    "body": ["# yapf: disable"]
  },

  "ipython debugger": {
    "prefix": ["ipdb"],
    "body": ["__import__('ipdb').set_trace()"]
  },

  "print": {
    "prefix": ["p"],
    "body": ["print(f'{$1=}')"]
  },

  "pretty print": {
    "prefix": ["pp"],
    "body": ["__import__('pprint').pprint($1)"]
  },

  "print sql": {
    "prefix": ["psql"],
    "body": [
      "print(__import__('sqlparse').format(str($1), reindent=True, keyword_case='upper'))"
    ]
  },

  "breakpoint": {
    "prefix": ["br"],
    "body": ["breakpoint()"]
  }
}
