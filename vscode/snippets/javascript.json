{
  "function": {
    "prefix": ["fn"],
    "body": ["function $1($2) {", "  $3", "}"]
  },

  "export function": {
    "prefix": ["efn"],
    "body": ["export function $1($2) {", "  $3", "}"]
  },

  "interface": {
    "prefix": ["in"],
    "body": ["interface $1 {", "  $2", "}"]
  },

  "export interface": {
    "prefix": ["ein"],
    "body": ["export interface $1 {", "  $2", "}"]
  },

  "export const": {
    "prefix": ["ec"],
    "body": ["export const $1"]
  },

  "export enum": {
    "prefix": ["ee"],
    "body": ["export enum $1 {", "  $2", "}"]
  },

  //* console.
  "console log": {
    "prefix": ["lg"],
    "body": ["console.log($1);"]
  },

  "console.dir": {
    "prefix": ["dir"],
    "body": ["console.dir($1);"]
  },

  //* prettier
  "prettier ignore": {
    "prefix": ["pig"],
    "body": ["// prettier-ignore"]
  },

  //* eslint.
  "eslint disable line": {
    "prefix": ["edl"],
    "body": ["// eslint-disable-line $1"]
  },

  "eslint disable next line": {
    "prefix": ["ednl"],
    "body": ["// eslint-disable-next-line $1"]
  },

  //* react.
  "React.useState": {
    "prefix": ["us"],
    "body": ["const [$1, $2] = React.useState($3);"]
  },

  "React.useEffect": {
    "prefix": ["ue"],
    "body": ["React.useEffect(() => {", "  $1", "}, [$2]);"]
  },

  "React.useRef": {
    "prefix": ["ur"],
    "body": ["const $1Ref = React.useRef<${2:HTMLDivElement}>(null);"]
  },

  "JSON.stringify": {
    "prefix": ["js"],
    "body": ["console.log($1, JSON.stringify($2, null, 2))"]
  },

  //* react-testing-library.
  "screen debug": {
    "prefix": ["sd"],
    "body": ["screen.debug($1)"]
  },

  "screen getByRole": {
    "prefix": ["sr"],
    "body": ["screen.getByRole('$1')"]
  },

  "screen getByText": {
    "prefix": ["st"],
    "body": ["screen.getByText('$1')"]
  },

  "screen getByLabelText": {
    "prefix": ["sl"],
    "body": ["screen.getByLabelText('$1')"]
  },

  "act": {
    "prefix": ["act"],
    "body": ["act(() => {", "  $1", "});"]
  },

  "await act": {
    "prefix": ["aact"],
    "body": ["await act(async () => {", "  $1", "});"]
  },

  //* jest.
  "describe": {
    "prefix": ["des"],
    "body": ["describe('$1', () => {", " ", "})"]
  },

  "test": {
    "prefix": ["ts"],
    "body": ["test('$1', () => {", "", "})"]
  },

  "async test": {
    "prefix": ["ats"],
    "body": ["test('$1', async () => {", "", "})"]
  },

  "beforeEach": {
    "prefix": ["bef"],
    "body": ["beforeEach(() => {", "  $1", "});"]
  }
}
