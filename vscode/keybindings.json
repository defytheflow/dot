[
  /**
   ** Switch focus between editor and panel with `ctrl+'` (no default bindings).
   */

  {
    "describe": "Focus panel",
    "key": "ctrl+'",
    "command": "workbench.action.focusPanel",
    "when": "!panelFocus"
  },
  {
    "describe": "Focus editor",
    "key": "ctrl+'",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "panelFocus"
  },

  /**
   ** Cycle through search results with `ctrl+n` and `ctrl+p` (default bindings use `f4` and `shift+f4`).
   **/

  {
    "key": "ctrl+n",
    "command": "search.action.focusNextSearchResult",
    "when": "!suggestWidgetVisible && (hasSearchResult || inSearchEditor)"
  },

  {
    "key": "ctrl+p",
    "command": "search.action.focusPreviousSearchResult",
    "when": "!suggestWidgetVisible && (hasSearchResult || inSearchEditor)"
  },

  /**
   ** Cycle through autocomplete options with `ctrl+n` and `ctrl+p` (default bindings use arrow keys).
   **/

  {
    "key": "ctrl+n",
    "command": "selectNextSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },

  {
    "key": "ctrl+p",
    "command": "selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },

  /**
   ** Overwrite vscode `isDevelopment` mode.
   **/

  {
    "describe": "Reload window",
    "key": "cmd+r",
    "command": "workbench.action.reloadWindow"
  },

  {
    "describe": "Toggle developer tools",
    "key": "cmd+alt+i",
    "command": "workbench.action.toggleDevTools"
  },

  /**
   ** Open source control view (default binding is `ctrl+shift+g`).
   **/

  {
    "describe": "Open Source Control",
    "key": "cmd+shift+g",
    "command": "workbench.view.scm"
  },

  /**
   ** Miscellaneous `cmd+g` bidings (most of them have no default bindings).
   **/

  {
    "describe": "Toggle activity bar visibility",
    "key": "cmd+g a",
    "command": "workbench.action.toggleActivityBarVisibility"
  },

  {
    "describe": "Toggle sidebar position",
    "key": "cmd+g b",
    "command": "workbench.action.toggleSidebarPosition"
  },

  {
    "desctibe": "Delete current file",
    "key": "cmd+g d",
    "command": "fileutils.removeFile"
  },

  {
    "describe": "Collapse all folders in the explorer",
    "key": "cmd+g e",
    "command": "workbench.files.action.collapseExplorerFolders"
  },

  {
    "describe": "Toggle full screen",
    "key": "cmd+g f",
    "command": "workbench.action.toggleFullScreen"
  },

  {
    "describe": "Toggle maximized panel",
    "key": "cmd+g j",
    "command": "workbench.action.toggleMaximizedPanel"
  },

  {
    "describe": "Toggle inlay hints",
    "key": "cmd+g h",
    "command": "settings.cycle.inlayHints"
  },

  {
    "describe": "Inspect Editor Tokens and Scopes",
    "key": "cmd+g i",
    "command": "editor.action.inspectTMScopes"
  },

  {
    "describe": "Toggle minimap",
    "key": "cmd+g m",
    "command": "editor.action.toggleMinimap"
  },

  {
    "describe": "Toggle problems",
    "key": "cmd+g p",
    "command": "workbench.actions.view.toggleProblems"
  },

  {
    "desctibe": "Rename current file",
    "key": "cmd+g r",
    "command": "fileutils.renameFile"
  },

  {
    "describe": "Configure User Snippets",
    "key": "cmd+g s",
    "command": "workbench.action.openSnippets"
  },

  {
    "describe": "Create terminal in editor area",
    "key": "cmd+g t",
    "command": "workbench.action.createTerminalEditor"
  },

  {
    "describe": "Open workspace settings",
    "key": "cmd+g w",
    "command": "workbench.action.openWorkspaceSettingsFile"
  },

  {
    "describe": "Center layout",
    "key": "cmd+g z",
    "command": "workbench.action.toggleCenteredLayout"
  },

  {
    "describe": "Open user settings (JSON)",
    "key": "cmd+g ,",
    "command": "workbench.action.openRawDefaultSettings"
  },

  {
    "describe": "Open user keyboard shortcuts (JSON)",
    "key": "cmd+g cmd+s",
    "command": "workbench.action.openGlobalKeybindingsFile"
  }

